<h2><%= t("subcontacts.edit.subcontacts_list") %></h2>
	<div class='class_list_edit_button'>
            <ul id='subcontacts'>
		<% @categoryall.subcontacts.order(:position).each{ |subcontact| %>
                    <%= content_tag_for :li, subcontact do %>
			    <%= subcontact_field.submit(subcontact.name, :name=>"#{@categoryall.id}_subcontact_#{subcontact.id}") %><%= subcontact_field.submit("x", :name=>"button_supsubcontact_#{subcontact.id}", :class=>"class_button_delete_right") %>
		    <% end %>
	        <% } %>
	    </ul>
	</div>
        <p>
 		<%= subcontact_field.label(:name) %><br>
		<%= subcontact_field.text_field(:name) %>
	</p>

	<p>
	<%= subcontact_field.submit(t(".add_new_mail_adress"), :name=>"#{@subcontact.id}-#{@categoryall.id}_addmail_button") %>
	</p>

