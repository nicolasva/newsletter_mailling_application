  <% if @mailstart.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@mailstart.errors.count, "error") %> prohibited this mailstart from being saved:</h2>

      <ul>
      <% @mailstart.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="actions">
	<%= render(:partial=>"/mailstarts/templates_mailstarts/mailstarts_form", :locals => {:mailstart_field => f}) %>

		<%= f.fields_for(:categoryalls, @categoryall) do |categoryall_field| %>
			<%= render(:partial=>"/categoryalls/templates_categoryalls/categoryalls_form", :locals => {:categoryall_field => categoryall_field}) %>
			<%= categoryall_field.fields_for(:subcontacts, @subcontact) do |subcontact_field| %>
				<%= render(:partial=>"/subcontacts/templates_subcontacts/subcontacts_form", :locals => {:subcontact_field => subcontact_field}) %>	

				<%= subcontact_field.fields_for(:mails, @mail) do |mail_field| %>
					<%= render(:partial=>"/mails/templates_mails/mails_form", :locals => {:mail_field=>mail_field}) %>
				<% end %>
			<% end %>
		<% end %>
  </div>
