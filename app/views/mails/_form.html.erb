  <% if @mail.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@mail.errors.count, "error") %> prohibited this mail from being saved:</h2>

      <ul>
      <% @mail.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="actions">
	<%= f.fields_for(:subcontacts, @subcontact) do |subcontact_field| %>
		<div class='div_class_mailstarts_categoryalls'>
		<%= subcontact_field.fields_for(:categoryalls, @categoryall) do |categoryall_field| %>
			<%= categoryall_field.fields_for(:mailstart, @mailstart) do |mailstart_field| %>
			<div class='div_class_mailstarts_categoryalls'>
				<%= render(:partial=>"/mailstarts/templates_mailstarts/mailstarts_form", :locals => {:mailstart_field => mailstart_field}) %>
			<% end %>
			<%= render(:partial=>"/categoryalls/templates_categoryalls/categoryalls_form", :locals => {:categoryall_field => categoryall_field}) %>
			</div>
	       <% end %>
	       </div>
	       <div class='div_class_subcontacts_mails'>
		       <%= render(:partial=>"/subcontacts/templates_subcontacts/subcontacts_form", :locals => {:subcontact_field => subcontact_field}) %>
	       </div>
	<% end %>
	<%= render(:partial=>"/mails/templates_mails/mails_form", :locals => {:mail_field=>f}) %>
  </div>
