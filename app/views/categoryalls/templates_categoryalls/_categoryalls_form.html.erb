          <h2>Liste des catégories appartenant a ce mail de départ</h2>
            <ul>       
	        <% @mailstart.categoryalls.each{ |categoryall| %>
		     <%= content_tag_for :li, categoryall do %>
			     <%= categoryall_field.submit(categoryall.name, :name=>"button_categoryall_#{categoryall.id}") %><%= categoryall_field.submit("x", :name=>"button_supcategoryall_#{categoryall.id}") %>
		     <% end %>
	        <% } %>
            </ul>
        <p>
		<%= categoryall_field.label(:name) %><br>
		<%= categoryall_field.text_field(:name) %>
	</p>

	<p>
		<%= categoryall_field.submit("Ajouter des nouveaux sous-contacts", :name=>"button_addsubcontact_#{@categoryall.id}") %>
	</p>


